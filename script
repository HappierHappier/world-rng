local a=game:GetService("HttpService")local b="used_ip_list.txt"local function c()if isfile(b)then local d=readfile(b)return a:JSONDecode(d)end;return{}end;local function e(f)local d=a:JSONEncode(f)writefile(b,d)end;local function g()local h,i=pcall(function()return game:HttpGet("https://api.ipify.org?format=json")end)if h then local j=a:JSONDecode(i)return j.ip else print("Failed to fetch IP address.")return nil end end;local k=c()local l=g()if l then if k[l]then print("Gotcha! You can't run this script again.")else k[l]=true;e(k)print("Script executed successfully for the first time.")local m={[1]=true,[2]="Antarctica"}game:GetService("ReplicatedStorage").Remotes.EquipRequest:InvokeServer(unpack(m))end else print("Unable to determine IP address.")end
